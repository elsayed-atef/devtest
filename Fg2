<Housekeeping>
    <ConfigFile>C:\housekeeping_config.xml</ConfigFile>
    <DryRun>false</DryRun>
    <SleepIntervalHours>5</SleepIntervalHours>
    
    <LogFile>
        <Path>D:\data\temp\housekeeping_log.txt</Path>
    </LogFile>
    
    <PathRule>
        <Path>C:\temp\logs</Path>
        <Enabled>True</Enabled>
        <RetentionDays>5</RetentionDays>
        <IncludeSubfolders>True</IncludeSubfolders>
        <Archive>
            <Enabled>True</Enabled>
            <Path>C:\archive</Path>
        </Archive>
        <ExcludeFolders>
            <ExcludeFolder>C:\temp\logs\exclude1</ExcludeFolder>
            <ExcludeFolder>C:\temp\logs\exclude2</ExcludeFolder>
        </ExcludeFolders>
    </PathRule>
    
    <PathRule>
        <Path>C:\temp\logs\keep</Path>
        <Enabled>True</Enabled>
        <RetentionDays>8</RetentionDays>
        <IncludeSubfolders>False</IncludeSubfolders>
        <Archive>
            <Enabled>False</Enabled>
            <Path>C:\archive</Path>
        </Archive>
        <ExcludeFolders />
    </PathRule>
    
    <MemoryDumpRules>
        <MemoryDumpRule>
            <Path>C:\Windows\Minidump</Path>
            <Enabled>True</Enabled>
            <RetentionDays>7</RetentionDays>
            <IncludeSubfolders>True</IncludeSubfolders>
        </MemoryDumpRule>
        <MemoryDumpRule>
            <Path>D:\ServerDumps</Path>
            <Enabled>True</Enabled>
            <RetentionDays>14</RetentionDays>
            <IncludeSubfolders>False</IncludeSubfolders>
        </MemoryDumpRule>
    </MemoryDumpRules>
</Housekeeping>
